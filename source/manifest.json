{
  "manifest_version": 2,
  "background": {
    "page": "html/background.html"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.vk.com/*"
      ],
      "css": [
        "css/style.css"
      ],
      "js": [
        "js/RumurMusicVK.js"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "lightweight vk.com audio downloader by Rumur for new version",
  "icons": {
    "128": "img/32.png",
    "16": "img/16.png",
    "48": "img/32.png"
  },
  "name": "Rumur vk audio downloader",
  "permissions": [
    "*://*.vk.com/*",
    "downloads"
  ],
  "version": "0.2.6",
  "web_accessible_resources": [
    "img/download.png"
  ]
}
